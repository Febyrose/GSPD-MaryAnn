      },
      "bundleIdentifier": "com.kemist2301.ocr"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.kemist2301.ocr"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "2a5a3888-5241-4ca3-985f-2ef19ca78f14"
      }
    }
  }
